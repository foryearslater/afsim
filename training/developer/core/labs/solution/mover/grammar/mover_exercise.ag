(rule stage-command
{
     fuel_mass  <Mass>
   | total_mass <Mass>
   | thrust     <Force>
   | burn_time  <Time>
   | address <string>
})

(struct MATLAB_BALLISTIC_MOVER
   :base_type Mover
   :symbol (type moverType MATLAB_BALLISTIC_MOVER)
{
     <Mover>
   | stage <integer> <stage-command>* end_stage
   | drag_coeff     <real>
   | effective_area <Area>
   | mass_payload   <Mass>
   | maxq           <real>
   | pitch_interval <Time>
   | vertical_time  <Time>
   | position       <Latitude> <Longitude>
   | heading        <Angle>
   | altitude       <Length>
   | pitch          <Angle>
})
