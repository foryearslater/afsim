<?xml version="1.0"?>
<devicelist>
  <device name="X3DPro">
    <id_string name="Logitech Extreme 3D*"/>
    <value name="roll">
      <axis name="0" deadzone="0.1"/>
    </value>
    <value name="pitch">
      <axis name="1" deadzone="0.1"/>
    </value>
    <value name="view_az_accum">
      <hat name="0" axis="h" value="-1,1"/>
    </value>
    <value name="view_el_accum">
      <hat name="0" axis="v" value="-1,1"/>
    </value>
    <value name="rudder">
      <axis name="2" value="-1,1"/>
    </value>
    <value name="throttle">
      <axis name="3" value="1,0"/>
    </value>
    <value name="afterburner">
      <toggle_button name="10" value="0,1"/>
    </value>
  </device>
</devicelist>
